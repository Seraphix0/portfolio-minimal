{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "Melvin Kusters - Portfolio",
    "titleTemplate": "%s · Portfolio Minimal",
    "description": "A collection of blogs I wrote about my contributions as a software engineer.",
    "author": "Melvin Kusters",
    "avatar": "../content/images/gatsby.png",
    "bio": "Dutch software engineer specializing in .NET development with 5+ years of experience.",
    "social": {
      "behance": "https://behance.com",
      "github": "https://github.com/Seraphix0",
      "linkedin": "https://www.linkedin.com/in/melvin-kusters/",
      "mail": "mailto:melvinkusters0@gmail.com"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "Melvin Kusters - Portfolio" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Blog", "url": "/blog" },
        { "label": "Highlights", "url": "/#highlights" },
        { "label": "Contact", "url": "/#contact" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "Resumé",
        "url": "/MelvinKustersExtendedCV.pdf"
      },
      "footer": [
        { "label": "Privacy", "url": "/privacy" },
        { "label": "Imprint", "url": "/imprint" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
